# Define your fizz(n) function below:
fizz <- function(n) {
result <- vector()
for (i in n) {
if (is.integer(i)==TRUE) {
ifelse (i%%3 == 0
, result <- c(result,"Fizz")
, result <- c(result, "")
)
}
else {stop("Not Integer")}
}
return(result)
}
# Use commands below to test your function:
fizz(5L)  # integer input
fizz(6L)  # integer input
fizz(5)   # non-integer input
buzz <- function(n) {
result <- vector()
for (i in n) {
if (is.integer(i)==TRUE) {
ifelse (i%%5 == 0
, result <- c(result,"Buzz")
, result <- c(result, "")
)
}
else {stop("Not Integer")}
}
return(result)
}
fizzbuzz <- function(n) {
result <- vector()
for (i in n) {
if (fizz(i)=="Fizz" & buzz(i)=="Buzz"){
result <- c(result, "Fizzbuzz")}
else if (fizz(i)=="Fizz"){
result <- c(result,"Fizz")}
else if (buzz(i)=="Buzz"){
result <- c(result,"Buzz")}
else {
result <- c(result,"")}
}
return(result)
}
fbr <- function(n,m) {
if (is.integer(n)==TRUE & is.integer(m)==TRUE) {
result <- c()
for (i in n:m){
ifelse (fizzbuzz(i)==""
, result <- c(result,i)
, result <- c(result,fizzbuzz(i))
)
}
return(result)
}
else (stop("Not Integer"))
}
